    "awesome-typescript-loader": "^5.2.1",
    "dts-bundle-generator": "^2.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
